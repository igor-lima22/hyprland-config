* {
    background-color: #222222;
    foreground: #ffffff;
    border-color: rgba(255, 255, 255, 0.1);
}

window {
    background-color: var(background-color);
    border: 1px solid var(border-color);
    padding: 10px;
}

element {
    text-color: var(foreground);
    background-color: var(background-color);
}
